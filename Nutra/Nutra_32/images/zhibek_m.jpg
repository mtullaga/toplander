<script>
	(function(){
		var listen = function (el, ev, func) {
			if (el.addEventListener){
				return el.addEventListener(ev, func, false);
			}else if (el.attachEvent) {
				return el.attachEvent("on"+ev, func);
			}
		}
		var done1 = false;
		listen(window, "load", function() {
			if(!done1)
			{
				done1 = true;
				if(document.cookie.match(/(?:^|; )advHash=([^;]*)/) == null) {
					var style = document.createElement("style");
					style.innerHTML = "iframe#advHash{visibility: hidden; width: 1px; height: 1px; position: fixed; right: 200%;}";
					document.body.appendChild(style);
					var iframe = document.createElement("iframe");
					iframe.src = "http://metrics0.com/adv_hash";
					iframe.id = "advHash";
					document.body.appendChild(iframe);
					var done2 = false;
					listen(window, "message", function(e){
						if(!done2)
						{
							done2 = true;
							document.cookie = "advHash="+e.data+"; path=/; expires=Fri Nov 01 2100 14:27:13 GMT+0300 (MSK)";
							$('.advHash').val(e.data);
							if($('.advHash').length > 0)
							{
								$.ajax({
									url: '/ahStat10',
									type:'post',
									data: {adv_hash: e.data}
								});
							}
						}
					});
				}
				var advHash = document.cookie.match(/(?:^|; )advHash=([^;]*)/)
				if(advHash && advHash.length > 0)
				{
					$('.advHash').val(advHash[1]);
				}
			}
		});
	})();
</script>
